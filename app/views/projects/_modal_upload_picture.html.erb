<!-- modal Upload Avatar -->
<div class="modal fade" id="uploadpicture-modal" tabindex="-1" role="dialog" aria-labelledby="upload picture" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
				<h1 class="modal-title text-center" id="">Edit Avatar</h1>
			</div>
			<div class="modal-body">
				<form role="form" id="form-uploadpicture">
					<div class="form-group">
						<label for="title-uploadpicture">Browse</label>
						<div class="input-group input-group-lg">
							<span class="input-group-addon"><i class="fa fa-folder-open-o"></i></span>
							<input type="file" class="form-control" id="title-uploadpicture" placeholder="Titre de projet">
						</div>
					</div><!-- /.form-group -->
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary btn-lg btn-block">Ok</button>
			</div>
		</div><!-- /.modal-content -->
	</div><!-- /.modal-dialog -->
</div><!-- /.modal --> 